<?xml version="1.0" encoding="utf-8"?>
<transfer default="chunk">

<section-def-cats>
  <def-cat n="nom">
     <cat-item tags="n.*"/>
     <cat-item tags="np.*"/>
  </def-cat>
  <def-cat n="n">
     <cat-item tags="n.*"/>
  </def-cat>
  <def-cat n="np">
     <cat-item tags="np.*"/>
  </def-cat>
  <def-cat n="num">
     <cat-item tags="num"/>
     <cat-item tags="num.*"/>
  </def-cat>
  <def-cat n="vblex">
     <cat-item tags="vblex.*"/>
  </def-cat>
  <def-cat n="vblex_pp_actv">
     <cat-item tags="vblex.*.pp.actv"/>
  </def-cat>
  <def-cat n="vblex_imp">
     <cat-item tags="vblex.impf.imp.p1.pl"/>
     <cat-item tags="vblex.perf.imp.p1.pl"/>
     <cat-item tags="vblex.dual.imp.p1.pl"/>
  </def-cat>
  <def-cat n="pprs_pasv">
     <cat-item tags="vblex.*.pprs.pasv.*"/>
  </def-cat>
  <def-cat n="prn">
     <cat-item tags="prn.dem.*"/>
     <cat-item tags="prn.itg.*"/>
     <cat-item tags="prn.rel.*"/>
     <cat-item tags="prn.ind.*"/>
     <cat-item tags="prn.def.*"/>
     <cat-item tags="prn.ref.*"/>
     <cat-item tags="prn.neg.*"/>
     <cat-item tags="prn.pers.*"/>
  </def-cat>
  <def-cat n="adj">
     <cat-item tags="adj.*"/>
  </def-cat>
  <def-cat n="adj_short">
     <cat-item tags="adj.*.short"/>
     <cat-item tags="adj.*.short.*"/>
     <cat-item tags="adj.short.*"/>
  </def-cat>
  <def-cat n="adj_comp">
     <cat-item tags="adj.sint.comp"/>
     <cat-item tags="adj.*.comp"/>
  </def-cat>
  <def-cat n="pr">
     <cat-item tags="pr"/>
  </def-cat>
  <def-cat n="adv">
     <cat-item tags="adv"/>
  </def-cat>
  <def-cat n="det">
     <cat-item tags="det"/>
     <cat-item tags="det.*"/>
  </def-cat>

  <def-cat n="adj_det">
     <cat-item tags="adj.*"/>
     <cat-item tags="det"/>
     <cat-item tags="det.*"/>
  </def-cat>
  <def-cat n="pp_pasv">
     <cat-item tags="vblex.impf.pp.pasv.*"/>
     <cat-item tags="vbhaver.impf.pp.pasv.*"/>
     <cat-item tags="vbser.impf.pp.pasv.*"/>
     <cat-item tags="vbmod.impf.pp.pasv.*"/>
  </def-cat>

</section-def-cats>

<section-def-attrs>
  <def-attr n="a_n">
     <attr-item tags="n"/>
  </def-attr>

  <def-attr n="a_np">
     <attr-item tags="np"/>
     <attr-item tags="np.ant"/>
     <attr-item tags="np.al"/>
     <attr-item tags="np.org"/>
     <attr-item tags="np.hyd"/>
     <attr-item tags="np.top"/>
     <attr-item tags="np.cog"/>
  </def-attr>

  <def-attr n="a_nom">
     <attr-item tags="n"/>
     <attr-item tags="np"/>
     <attr-item tags="np.ant"/>
     <attr-item tags="np.al"/>
     <attr-item tags="np.org"/>
     <attr-item tags="np.hyd"/>
     <attr-item tags="np.top"/>
     <attr-item tags="np.cog"/>
  </def-attr>

  <def-attr n="a_prp">
     <attr-item tags="pr"/>
  </def-attr>

  <def-attr n="a_adj">
     <attr-item tags="adj"/>
     <attr-item tags="adj.sint"/>
  </def-attr>

  <def-attr n="a_vrb">
     <attr-item tags="vblex"/>
  </def-attr>

  <def-attr n="trans">
     <attr-item tags="tv"/>
     <attr-item tags="iv"/>
  </def-attr>

  <def-attr n="asp">
     <attr-item tags="impf"/>
     <attr-item tags="dual"/>
     <attr-item tags="perf"/>
  </def-attr>

  <def-attr n="tns">
     <attr-item tags="inf"/>
     <attr-item tags="pp.pasv"/>
     <attr-item tags="pp.actv"/>
     <attr-item tags="pp.adv"/>
     <attr-item tags="pprs.pasv"/>
     <attr-item tags="pprs.actv"/>
     <attr-item tags="pprs.adv"/>
     <attr-item tags="fut"/>
     <attr-item tags="pres"/>
     <attr-item tags="imp"/>
     <attr-item tags="past"/>
  </def-attr>

  <def-attr n="a_prn">
     <attr-item tags="prn"/>
     <attr-item tags="prn.itg"/>
     <attr-item tags="prn.pers"/>
     <attr-item tags="prn.neg"/>
     <attr-item tags="prn.def"/>
     <attr-item tags="prn.ref"/>
     <attr-item tags="prn.ind"/>
     <attr-item tags="prn.dem"/>
     <attr-item tags="prn.rel"/>
     <attr-item tags="prn.qnt"/>
  </def-attr>

  <def-attr n="a_det">
     <attr-item tags="det"/>
     <attr-item tags="det.ord"/>
     <attr-item tags="det.pos"/>
     <attr-item tags="det.dem"/>
     <attr-item tags="det.ind"/>
     <attr-item tags="det.itg"/>
  </def-attr>

  <def-attr n="a_sh">
    <attr-item tags="short"/>
  </def-attr>

  <def-attr n="gen">
     <attr-item tags="m"/>
     <attr-item tags="f"/>
     <attr-item tags="nt"/>
     <attr-item tags="mf"/>
     <attr-item tags="mfn"/>
  </def-attr>

  <def-attr n="prs">
     <attr-item tags="p1"/>
     <attr-item tags="p2"/>
     <attr-item tags="p3"/>
  </def-attr>

  <def-attr n="anim">
     <attr-item tags="nn"/>
     <attr-item tags="aa"/>
     <attr-item tags="an"/>
  </def-attr>


  <def-attr n="nbr">
     <attr-item tags="sg"/>
     <attr-item tags="sp"/>
     <attr-item tags="pl"/>
  </def-attr>

  <def-attr n="dbl_nbr">
     <attr-item tags="sg.sg"/>
     <attr-item tags="sg.pl"/>
     <attr-item tags="pl.sg"/>
     <attr-item tags="pl.pl"/>
  </def-attr>


  <def-attr n="cas">
     <attr-item tags="nom"/>
     <attr-item tags="acc"/>
     <attr-item tags="dat"/>
     <attr-item tags="gen"/>
     <attr-item tags="loc"/>
     <attr-item tags="ins"/>
     <attr-item tags="voc"/>
     <attr-item tags="prp"/>
  </def-attr>
</section-def-attrs>

<section-def-vars>
  <def-var n="number"/>
  <def-var n="genere"/>
</section-def-vars>

<section-def-macros>
  <!-- Case adapter: loc (bel) → prp (rus) -->
  <!-- nbr adapter: removes extranious numbers created by mandatory bidix pl or sg tags. -->
  <def-macro n="case_adapter1" npar="1">
    <choose>
      <when>
        <test><equal><clip pos="1" side="tl" part="cas"/><lit-tag v="loc"/></equal></test>
        <let><clip pos="1" side="tl" part="cas"/><lit-tag v="prp"/></let>
      </when>
    </choose>
    <choose>
      <when>
        <test>
          <or>
            <equal><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="sg.sg"/></equal>
            <equal><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="sg.pl"/></equal>
          </or>
        </test>
        <let><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="sg"/></let>
      </when>
      <when>
        <test>
          <or>
            <equal><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="pl.sg"/></equal>
            <equal><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="pl.pl"/></equal>
          </or>
        </test>
        <let><clip pos="1" side="tl" part="dbl_nbr"/><lit-tag v="pl"/></let>
      </when>
    </choose>
  </def-macro>

  <!-- Transfer gender, number -->
  <def-macro n="case_adapter_n" npar="2">
    <choose>
      <when>
        <test>
          <and>
            <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="sg"/></equal>
            <equal><clip pos="2" side="tl" part="nbr"/><lit-tag v="sg"/></equal>
          </and>
        </test>
        <let>
          <clip pos="1" side="tl" part="gen"/>
          <clip pos="2" side="tl" part="gen"/>
        </let>
      </when>
      <when>
        <test>
          <equal><clip pos="2" side="tl" part="nbr"/><lit-tag v="pl"/></equal>
        </test>
        <let>
          <clip pos="1" side="tl" part="nbr"/>
          <clip pos="2" side="tl" part="nbr"/>
        </let>
        <let>
          <clip pos="1" side="tl" part="gen"/>
          <lit-tag v="mfn"/>
        </let>
      </when>
    </choose>
  </def-macro>

</section-def-macros>

<section-rules>

  <rule comment="RULE: Past passive participle → Present passive participle">
    <pattern>
      <pattern-item n="pp_pasv"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="anim"/><lit-tag v=""/></equal></test>
          <let><clip pos="1" side="tl" part="anim"/><lit-tag v="an"/></let>
        </when>
      </choose>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="cas"/><lit-tag v=""/></equal></test>
          <let><clip pos="1" side="tl" part="cas"/><lit-tag v="nom"/></let>
        </when>
      </choose>
       <out>
        <chunk name="prtcpl" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SA"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_vrb"/>
            <clip pos="1" side="tl" part="asp"/>
            <clip pos="1" side="tl" part="trans"/>
            <lit-tag v="pprs.pasv"/>
            <clip pos="1" side="tl" part="gen"/>
            <clip pos="1" side="tl" part="anim"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="cas"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  
  <rule comment="RULE: Determiner → Determiner">
    <pattern>
      <pattern-item n="det"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <out>
        <chunk name="det" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="DET"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  <rule comment="RULE: Numeral → Numeral">
    <pattern>
      <pattern-item n="num"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <out>
        <chunk name="num" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="NUM"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  <rule comment="RULE: Pronoun → Pronoun">
    <pattern>
      <pattern-item n="prn"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <out>
        <chunk name="prn" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="PRN"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  <rule comment="RULE: Noun → Noun">
    <pattern>
      <pattern-item n="n"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <out>
        <chunk name="n" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SN"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  <rule comment="RULE: Proper Noun → Proper Noun">
    <pattern>
      <pattern-item n="np"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <out>
        <chunk name="np" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SN"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_np"/>
            <clip pos="1" side="tl" part="gen"/>
            <clip pos="1" side="tl" part="nbr"/>
            <clip pos="1" side="tl" part="cas"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>


  <rule comment="RULE: Verb imp → Давай + p1 pl">
    <pattern>
      <pattern-item n="vblex_imp"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <choose>
        <when>
          <test><equal><clip pos="1" side="tl" part="asp"/><lit-tag v="impf"/></equal></test>
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="pres"/></let>
        </when>
        <when>
          <test><equal><clip pos="1" side="tl" part="asp"/><lit-tag v="perf"/></equal></test>
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="fut"/></let>
        </when>
      </choose>
      <out>
        <chunk name="vb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
          </tags>
          <lu>
            <lit v="давай"/>
            <lit-tag v="ij"/>
          </lu>
          <b/>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="tags"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>

  <rule comment="RULE: Verb pp actv → Verb pp adv">
    <pattern>
      <pattern-item n="vblex_pp_actv"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <let><clip pos="1" side="tl" part="tns"/><lit-tag v="pp.adv"/></let>
      <out>
        <chunk name="vb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="tags"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>


  <rule comment="RULE: Verb → Verb">
    <pattern>
      <pattern-item n="vblex"/>
    </pattern>
    <action>
      <call-macro n="case_adapter1"><with-param pos="1"/></call-macro>
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="tl" part="asp"/><lit-tag v="impf"/></equal>
              <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="fut"/></equal>
            </and>
          </test>          
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="pres"/></let>
        </when>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="tl" part="asp"/><lit-tag v="perf"/></equal>
              <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="pres"/></equal>
            </and>
          </test>          
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="fut"/></let>
        </when>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="tl" part="asp"/><lit-tag v="perf"/></equal> <!-- temp solution -->
              <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="pprs.adv"/></equal>
            </and>
          </test>          
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="pp.adv"/></let>
        </when>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="tl" part="asp"/><lit-tag v="impf"/></equal> <!-- temp solution -->
              <equal><clip pos="1" side="tl" part="tns"/><lit-tag v="pp.adv"/></equal>
            </and>
          </test>          
          <let><clip pos="1" side="tl" part="tns"/><lit-tag v="pprs.adv"/></let>
        </when>
      </choose>
      <out>
        <chunk name="vb" case="caseFirstWord">
          <tags>
            <tag><lit-tag v="SV"/></tag>
          </tags>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="tags"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
        </chunk>
      </out>
    </action>
  </rule>


